<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yunshan.tech.dao.ProductMapper">

    <select id="selectAll" resultType="com.yunshan.tech.model.Product">
        SELECT id, name, images, specification, description, `product_condition`, price, contact
        FROM Product
    </select>

</mapper>
